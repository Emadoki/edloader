<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="EdLoader">
        <attr name="loader_type" format="enum">
            <enum name="classic" value="0"/>
            <enum name="grow" value="1"/>
            <enum name="swap" value="2"/>
            <enum name="jump" value="3"/>
            <enum name="pacman" value="4"/>
        </attr>
        <attr name="loader_radius" format="dimension"/>
        <attr name="loader_margin" format="dimension"/>
        <attr name="loader_amount" format="integer"/>
        <attr name="loader_color" format="color"/>
        <attr name="loader_speed" format="float"/>
    </declare-styleable>
</resources>